Begin Class Name=Pyrotechnics
   Begin Text
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
Class Pyrotechnics Expands Root Intrinsic

EnumDef ERampEffect = RAMP_Down, RAMP_Up, RAMP_UpThenDown

Dim GravityMult as Editable Real
Dim AccelerationFactor As Editable Real  ' Velocity multipler (each tick)
Dim Noise As Editable Byte ' Noise generated by effect
Dim InitialSound As Editable Sound ' Sound generated when sound is spawned.
Dim Textures(20) As Editable Texture ' A sequence of animating textures - the first TextureCount are used.
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
   End Text
   Begin DefaultProperties
      Class=Pyrotechnics
      bTemplateClass=False
      LifeSpan=35
      AccelerationFactor=1.0
      Noise=0
      InitialSound=None
	  DefaultEdCategory=Pyrotechnics
   End DefaultProperties
End Class

